<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="awagadro.studentApp.entity">
    <class name="ApplicantResult" table="applicant_result">
        <id name="applicantResultId" column="applicant_result_id">
            <generator class="native"/>
        </id>
        <many-to-one name="applicant" column="applicant_id" class="awagadro.studentApp.entity.Applicant"/>
        <many-to-one name="subject" column="subject_id" class="awagadro.studentApp.entity.Subject"/>
        <property name="mark" column="mark"/>
    </class>
</hibernate-mapping>